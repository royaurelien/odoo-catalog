    <!-- Dashboard views -->
    <record id="view_git_organization_kanban" model="ir.ui.view">
        <field name="name">git.organization.kanban.inherited</field>
        <field name="model">git.organization</field>
        <field name="inherit_id" ref="catalog.view_git_organization_kanban"/>
        <field name="arch" type="xml">
            <xpath expr="//kanban" position="attributes">
                <attribute name="js_class">catalog_kanban_dashboard</attribute>
                <attribute name="sample">0</attribute>
                <attribute name="group_create">0</attribute>
                <!-- <attribute name="class">oe_background_grey o_kanban_dashboard o_catalog_kanban</attribute> -->
            </xpath>
        </field>
    </record>

    <record id="view_git_organization_tree" model="ir.ui.view">
        <field name="name">git.organization.tree.inherited</field>
        <field name="model">git.organization</field>
        <field name="inherit_id" ref="catalog.view_git_organization_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="class">o_purchase_order</attribute>
                <attribute name="js_class">catalog_list_dashboard</attribute>
                <attribute name="sample">0</attribute>
            </xpath>
        </field>
    </record>


    <!-- Dashboard action buttons -->
    <!-- Dashboard action buttons: End in List view -->
    <record id="catalog_action_dashboard_list" model="ir.actions.act_window">
        <field name="name">Custom Addons</field>
        <field name="res_model">custom.addon</field>
        <field name="view_mode">form,tree,kanban</field>
        <field name="view_id" ref="catalog.view_custom_addon_tree"/>
        <field name="search_view_id" ref="catalog.view_custom_addon_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No addons to display
            </p>
        </field>
    </record>

    <record id="catalog_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Custom Addons</field>
        <field name="res_model">custom.addon</field>
        <field name="view_mode">form,kanban,tree</field>
        <field name="view_id" ref="catalog.custom_addon_view_kanban"/>
        <field name="search_view_id" ref="catalog.view_custom_addon_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No addons to display
            </p>
        </field>
    </record>


    <record id="catalog_git_organization_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Git Organization</field>
        <field name="res_model">git.organization</field>
        <field name="view_mode">form,kanban,tree</field>
        <field name="view_id" ref="catalog_dashboard.view_git_organization_kanban"/>
        <field name="search_view_id" ref="catalog.view_git_organization_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No organization to display
            </p>
        </field>
    </record>

    <record id="catalog_git_organization_action_dashboard_list" model="ir.actions.act_window">
        <field name="name">Git Organization</field>
        <field name="res_model">git.organization</field>
        <field name="view_mode">form,tree,kanban</field>
        <field name="view_id" ref="catalog_dashboard.view_git_organization_tree"/>
        <field name="search_view_id" ref="catalog.view_git_organization_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No organization to display
            </p>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_view_dashboard">
      <field name="name">Dashboard</field>
      <field name="res_model">git.organization</field>
      <field name="view_mode">kanban,tree,form</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
          No organization to display
        </p>
      </field>
    </record>


    <menuitem name="Dashboard" id="menu_dasboard"
              parent="catalog.menu_custom_addons_root"
              sequence="5"
              action="action_view_dashboard"/>

  </data>
</odoo>