    <!-- Dashboard action buttons: Git Organization -->
    <record id="catalog_git_organization_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Git Organization</field>
        <field name="res_model">git.organization</field>
        <field name="view_mode">form,kanban,tree</field>
        <!-- <field name="view_id" ref="catalog_dashboard.view_dashboard_kanban"/> -->
        <field name="view_ids" eval="[
            (5, 0, 0),
            (0, 0, {'view_mode': 'kanban', 'view_id': ref('catalog_dashboard.view_dashboard_kanban')}),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('catalog_dashboard.view_dashboard_tree')}),
            (0, 0, {'view_mode': 'form'})]"/>
        <field name="search_view_id" ref="catalog.view_git_organization_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No organization to display
            </p>
        </field>
    </record>

    <record id="catalog_git_organization_action_dashboard_list" model="ir.actions.act_window">
        <field name="name">Git Organization</field>
        <field name="res_model">git.organization</field>
        <field name="view_mode">form,tree,kanban</field>
        <!-- <field name="view_id" ref="catalog_dashboard.view_dashboard_tree"/> -->
        <field name="view_ids" eval="[
            (5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('catalog_dashboard.view_dashboard_tree')}),
            (0, 0, {'view_mode': 'kanban', 'view_id': ref('catalog_dashboard.view_dashboard_kanban')}),
            (0, 0, {'view_mode': 'form'})]"/>
        <field name="search_view_id" ref="catalog.view_git_organization_search"/>
        <field name="target">main</field>
