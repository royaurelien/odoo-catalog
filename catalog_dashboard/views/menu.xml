    <record id="catalog_branch_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Git Branch</field>
        <field name="res_model">git.branch</field>
        <field name="view_mode">form,kanban,tree</field>
        <field name="view_id" ref="catalog.view_git_branch_tree"/>
