        </field>
    </record>

    <record id="catalog_repository_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Git Repository</field>
        <field name="res_model">git.repository</field>
        <field name="view_mode">form,kanban,tree</field>
        <field name="view_id" ref="catalog.view_git_repository_kanban"/>
        <field name="search_view_id" ref="catalog.view_git_repository_search"/>
