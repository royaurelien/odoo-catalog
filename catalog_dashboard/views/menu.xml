            <p class="o_view_nocontent_empty_folder">
                No organization to display
            </p>
        </field>
    </record>

    <!-- Dashboard -->
    <record model="ir.actions.act_window" id="action_view_dashboard">
      <field name="name">Dashboard</field>
      <field name="res_model">git.organization</field>
      <!-- <field name="view_id" ref="catalog_dashboard.view_dashboard_kanban"/> -->
        <field name="view_ids" eval="[
            (5, 0, 0),
            (0, 0, {'view_mode': 'kanban', 'view_id': ref('catalog_dashboard.view_dashboard_kanban')}),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('catalog_dashboard.view_dashboard_tree')}),
            (0, 0, {'view_mode': 'form'})]"/>
      <field name="view_mode">kanban,tree,form</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
          No organization to display
        </p>
      </field>
    </record>

    <record model="ir.actions.act_window" id="catalog.action_view_organization">
      <field name="view_id" ref="catalog.view_git_organization_kanban"/>
        <field name="view_ids" eval="[
            (5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('catalog.view_git_organization_tree')}),
            (0, 0, {'view_mode': 'kanban', 'view_id': ref('catalog.view_git_organization_kanban')}),
            (0, 0, {'view_mode': 'form'})]"/>
    </record>


    <menuitem name="Dashboard" id="menu_dasboard"
              parent="catalog.menu_custom_addons_root"
              sequence="5"
              action="action_view_dashboard"/>

  </data>
</odoo>