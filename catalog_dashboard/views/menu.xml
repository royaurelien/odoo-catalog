<odoo>
  <data>

    <!-- Dashboard action buttons -->
    <!-- Dashboard action buttons: Custom Addons -->
    <record id="catalog_action_dashboard_list" model="ir.actions.act_window">
        <field name="name">Custom Addons</field>
        <field name="res_model">custom.addon</field>
        <field name="view_mode">form,tree,kanban</field>
        <field name="view_id" ref="catalog.view_custom_addon_tree"/>
        <field name="search_view_id" ref="catalog.view_custom_addon_search"/>
        <field name="target">main</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
                No addons to display
            </p>
        </field>
    </record>

    <record id="catalog_action_dashboard_kanban" model="ir.actions.act_window">
        <field name="name">Custom Addons</field>
        <field name="res_model">custom.addon</field>
        <field name="view_mode">form,kanban,tree</field>
        <field name="view_id" ref="catalog.custom_addon_view_kanban"/>
        <field name="search_view_id" ref="catalog.view_custom_addon_search"/>
