<odoo>
    <data>

        <record model="ir.config_parameter" id="force_update">
            <field name="key">catalog.force_update</field>
            <field name="value">False</field>
        </record>

        <record model="ir.config_parameter" id="sync_delay">
            <field name="key">catalog.sync_delay</field>
            <field name="value">3600</field>
        </record>

        <record model="ir.config_parameter" id="odoo_major_version">
            <field name="key">catalog.odoo_major_version</field>
            <field name="value">^(0|[1-9]\d*)\.(0|[1-9]\d*)$</field>
        </record>

        <record model="ir.config_parameter" id="enable_icon_search">
            <field name="key">catalog.enable_icon_search</field>
            <field name="value">True</field>
        </record>

        <record model="ir.config_parameter" id="enable_test">
            <field name="key">catalog.enable_test</field>
            <field name="value">False</field>
        </record>

        <record model="ir.config_parameter" id="limit_test">
            <field name="key">catalog.limit_test</field>
            <field name="value">10</field>
        </record>

    </data>

</odoo>
