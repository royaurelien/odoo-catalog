<odoo>
  <data>

    <record id="view_git_repository_form" model="ir.ui.view">
        <field name="name">view.git.repository.form.inherited</field>
        <field name="model">git.repository</field>
        <field name="inherit_id" ref="catalog.view_git_repository_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_view_contributor']" position="before">
              <button
                  type="object"
                  class="oe_stat_button"
                  icon="fa-plug"
                  name="action_view_webhook">
                  <div class="o_stat_info">
                      <field name="webhook_count" class="o_stat_value"/>
                      <span class="o_stat_text"> Webhook</span>
                  </div>
              </button>
            </xpath>
        </field>
    </record>

  </data>
</odoo>